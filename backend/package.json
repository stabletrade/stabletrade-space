{
  "name": "test-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "infra:up": "docker-compose up -d postgres",
    "infra:down": "docker-compose stop postgres",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run build && npx typeorm migration:run",
    "migration:generate": "npm run build && npm run typeorm -- migration:generate -n",
    "migration:revert": "npm run build && npx typeorm migration:revert",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:one": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretest:e2e": "docker-compose up -d postgresTest",
    "posttest:e2e": "docker-compose stop postgresTest && docker-compose rm -f postgresTest",
    "prepare": "husky install",
    "reinstall": "docker-compose stop postgres && docker-compose rm -f postgres && npm run infra:up && npm run migration:run && npm run seed:run "
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^4.0.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.9",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.9",
    "@types/lodash": "^4.17.5",
    "async-retry": "^1.3.3",
    "axios": "^1.7.2",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.1.2",
    "bs58": "^6.0.0",
    "cache-manager": "^5.6.1",
    "cache-manager-ioredis-yet": "^2.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.4.5",
    "got": "^14.4.8",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "minio": "^8.0.0",
    "nestjs-minio-client": "^2.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.3.0",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.2.45",
    "uuid": "^11.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/async-retry": "^1.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "lint-staged": "^12.3.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix --max-warnings 100"
  }
}
